{
    "name": "xorgxx/neox-dashboard-bundle",
    "description": "Neox dashboard bundle for Symfony",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "symfony-ux"
    ],
    "authors": [
        {
            "name": "Xorg",
            "email": "xorg@i2p.i2p"
        }
    ],
    "autoload": {
        "psr-4": {
            "NeoxDashBoard\\NeoxDashBoardBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NeoxDashBoard\\NeoxDashBoardBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "twig": {
            "paths": {
                "NeoxDashBoard\\NeoxDashBoardBundle\\src\\Templates\\": "NeoxDashBoardBundle"
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.6 || ^3.0",
        "gedmo/doctrine-extensions": "^3.17",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony/amqp-messenger": "^6.0 || ^7.1",
        "symfony/asset": "^6.3|^7.0",
        "symfony/asset-mapper": "^6.3|^7.0",
        "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "^6.0 ||^7.1",
        "symfony/twig-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/uid": "^5.4 || ^6.0 || ^7.0",
        "symfony/ux-icons": "^2.0",
        "symfony/ux-live-component": "^2.0",
        "symfony/ux-turbo": "^2.0",
        "symfony/ux-twig-component": "*",
        "symfony/validator": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^6.3|^7.0",
        "twig/twig": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.4 || ^6.0 || ^7.0",
        "symfony/dotenv": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/security-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/mime": "^5.4 || ^6.0 || ^7.0",
        "psr/log": "^1.0 | ^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "zenstruck/foundry": "^2.0"
    },
    "config": {
        "sort-packages": true
    }
}
