{#{% include '@NeoxDashBoardBundle/components/BootstrapModal.html.twig' %}#}
<div {{ attributes }} >
    <div class="pb-5" style="background-color: {{ NeoxDashSetup.theme }}">
        <div class=" sticky-top">
            <turbo-frame id="NeoxDashHeader" >
                <twig:NeoxDashHeader :NeoxDashSetup="NeoxDashSetup" id="live-NeoxDashHeader"/>
            </turbo-frame>
            {#    {% include '@NeoxDashBoardBundle/_Partials/header/Inc-Header.html.twig' %}#}
        </div>
        <div class="container">
            {#    <div class="accordion" id="accordionExample">#}
            <div id="NeoxDashBoardContent" >
                {% for class in  NeoxDashSetup.class %}
                    <turbo-frame id="NeoxDashBoardContent-{{ class.id }}">
                        <twig:NeoxDashBoardContent :NeoxDashClass="class" :loopn="loop"  id="live-NeoxDashBoardContent@{{ class.id }}"/>
                    </turbo-frame>
                {% endfor %}
            </div>

            {#    </div>#}
        </div>
    </div>
</div>





