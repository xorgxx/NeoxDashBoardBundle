{#      BULDER menu fast ====   #}

<section {{ attributes }} class="py-3">

    <input
        id="delayedInput"
        type="search"
        data-model="query"
        aria-describedby="basic-addon2"
        placeholder="Search link ...."
        class="bg-white fw-bold bg-opacity-50 form-control"
    />
    {% set height = section.heigth|default('2.8') ~ 'rem' %}
    {% set classObjet   = classObjet %}

    <div class="container" >
        <div class="row pb-2" >
            {% for item in computed.packages %}
                {% set section      = item.section %}
                {% set neoxSearch   = "false" %}
                <div class="justify-content-start col-3 g-3" >
                    {{ include('@NeoxDashBoardBundle/_Partials/dashBoardContent/_TabsCardItem.html.twig') }}
                </div >
            {% else %}
                <div class="col-12" >
                    <p >No packages found for "{{ query }}"</p >
                </div >
            {% endfor %}
        </div >
    </div >


</section >