{
    "name": "@xorgxx/neox-dashboard-bundle",
    "description": "Xorgxx| Neox Dash board integration for Symfony",
    "license": "MIT",
    "version": "1.0.0",
    "main": "dist/neox_modal_controller.js",
    "scripts": {
        "enabled": true
    },
    "symfony": {
        "controllers": {
            "@symfony/ux-autocomplete": {
                "autocomplete": {
                    "enabled": true,
                    "fetch": "eager",
                    "autoimport": {
                        "tom-select/dist/css/tom-select.default.css": true,
                        "tom-select/dist/css/tom-select.bootstrap4.css": false,
                        "tom-select/dist/css/tom-select.bootstrap5.css": false
                    }
                }
            },
            "@symfony/ux-live-component": {
                "live": {
                    "enabled": true,
                    "fetch": "eager",
                    "autoimport": {
                        "@symfony/ux-live-component/dist/live.min.css": true
                    }
                }
            },
            "@symfony/ux-notify": {
                "notify": {
                    "enabled": true,
                    "fetch": "eager"
                }
            },
            "@symfony/ux-turbo": {
                "turbo-core": {
                    "enabled": true,
                    "fetch": "eager"
                },
                "mercure-turbo-stream": {
                    "enabled": true,
                    "fetch": "eager"
                }
            },
            "@symfony/ux-typed": {
                "typed": {
                    "enabled": true,
                    "fetch": "eager"
                }
            },
            "neox_modal": {
                "main": "dist/neox_modal_controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "neox_drag_nav": {
                "main": "dist/neox_drag_nav_controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "neox_drag_drop": {
                "main": "dist/neox_drag_drop_controller.js",
                "fetch": "eager",
                "enabled": true
            }
        },
        "assets": {
            "path": "assets"
        },
        "importmap": {
            "@symfony/ux-live-component": "^3.0",
            "@hotwired/stimulus": "^3.0",
            "@symfony/stimulus-bridge": "^3.0",
            "@hotwired/turbo" : "^8.0",
            "@popperjs/core" : "^2.11",
            "jquery": "^3.6",
            "bootstrap": "^5.0",
            "bootstrap/dist/css/bootstrap.min.css": "^5.0",
            "sweetalert2": "^11.0",
            "@sweetalert2/theme-dark/dark.css": "^5.0",
            "@sweetalert2/theme-bootstrap-4/bootstrap-4.css": "^5.0",
            "sweetalert2/dist/sweetalert2.esm.min": "^11.0",
            "sweetalert2/dist/sweetalert2.esm.all.min": "^11.0",
            "tom-select/dist/css/tom-select.default.css" : "^2.0"
        }
    },
    "peerDependencies": {
        "tom-select/dist/css/tom-select.default.css" : "^2.0",
        "symfony/stimulus-bundle": "^3.0",
        "@symfony/stimulus-bridge": "^3.0",
        "@symfony/ux-live-component": "^3.0",
        "@hotwired/stimulus": "^3.0",
        "bootstrap": "^5.0",
        "sweetalert2": "^11.0"
    },
    "devDependencies": {}
}