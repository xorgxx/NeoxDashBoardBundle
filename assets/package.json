            "@sweetalert2/theme-bootstrap-4/bootstrap-4.css": "^5.0",
            "sweetalert2/dist/sweetalert2.esm.min": "^11.0",
            "sweetalert2/dist/sweetalert2.esm.all.min": "^11.0",
